{
    "version": 2,
    "buildCommand": "bash build_files.sh",
    "builds": [
      {
        "src": "web_agent/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python" }
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "manage.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/bash"
      },
      {
        "src": "manage.py tailwind build",
        "use": "@vercel/python"
      }
      

      
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "web_agent/wsgi.py"
      }
    ]
  }
  